KAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,IAAA,IAAA,CAAA,CAAA,CAAA,IAAA,IAAA,IAAA,IAAA,CAAA,CAAA,CAAA,IAAA,CAAA;AACA,OAFA;;AAIA,WAAA,SAAA,GAAA,UAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KAzEA;AA2EA,IAAA,WA3EA,yBA2EA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,CAAA,CAAA;AACA,KA9EA;AAgFA,IAAA,iBAhFA,6BAgFA,QAhFA,EAgFA,KAhFA,EAgFA;AACA,WAAA,eAAA,GAAA,QAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KAnFA;AAqFA,IAAA,kBArFA,gCAqFA;AAAA;;AACA,MAAA,mBAAA,CAAA,SAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,WAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA,KA7FA;AA8FA,IAAA,cA9FA,0BA8FA,GA9FA,EA8FA;AAAA;;AACA,MAAA,mBAAA,CAAA,MAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,aAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA,KAtGA;AAuGA,IAAA,WAvGA,yBAuGA;AACA;AACA,UAAA,OAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,IAAA,aAAA,OAAA,CAAA,WAAA,EAAA,cACA,OAAA,CAAA,QAAA,KAAA,CADA,cAEA,OAAA,CAAA,OAAA,EAFA,CAAA;AAGA,aAAA,IAAA;AACA;AA9GA,GAvBA;AAuIA,EAAA,OAvIA,qBAuIA;AACA,IAAA,mBAAA,CAAA,cAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,EAAA,CACA,OADA,EAEA,KAAA,YAFA;AAIA,GA5IA;AA6IA,EAAA,aA7IA,2BA6IA;AACA,IAAA,mBAAA,CAAA,cAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GAAA,CACA,OADA,EAEA,KAAA,YAFA;AAIA,GAlJA;AAmJA,EAAA,QAAA,oBACA,UAAA,CAAA;AACA,IAAA,IAAA,EAAA;AADA,GAAA,CADA;AAnJA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"center\" style=\"margin-left: 50px\">\r\n      <br>\r\n      <H3>Today's Orders</H3>\r\n      <vs-table v-model=\"selected\">\r\n        <template #header>\r\n          <vs-input v-model=\"search1\" border placeholder=\"Search\" />\r\n        </template>\r\n        <template #thead>\r\n          <vs-tr>\r\n            <vs-th\r\n              sort\r\n              @click=\"tutorials = $vs.sortData($event, tutorials, 'name')\"\r\n            >\r\n              Name\r\n            </vs-th>\r\n            <vs-th\r\n              sort\r\n              @click=\"tutorials = $vs.sortData($event, tutorials, 'hpNumber')\"\r\n            >\r\n              Phone Number\r\n            </vs-th>\r\n            <vs-th\r\n              sort\r\n              @click=\"\r\n                tutorials = $vs.sortData($event, tutorials, 'orderNumber')\r\n              \"\r\n            >\r\n              Order Number\r\n            </vs-th>\r\n            <vs-th\r\n              sort\r\n              @click=\"tutorials = $vs.sortData($event, tutorials, 'option')\"\r\n            >\r\n              Option\r\n            </vs-th>\r\n            <vs-th\r\n              sort\r\n              @click=\"tutorials = $vs.sortData($event, tutorials, 'time')\"\r\n            >\r\n              Time\r\n            </vs-th>\r\n            <vs-th\r\n              sort\r\n              @click=\"tutorials = $vs.sortData($event, tutorials, 'date')\"\r\n            >\r\n              Date\r\n            </vs-th>\r\n            <vs-th>\r\n              <vs-checkbox\r\n                :indeterminate=\"selected1.length == tutorials.length\"\r\n                v-model=\"allCheck\"\r\n                @change=\"selected = $vs.checkAll(selected, tutorials)\"\r\n              />\r\n            </vs-th>\r\n            <vs-th>\r\n              <vs-button danger @click=\"removeAllTutorials\"> X All </vs-button>\r\n            </vs-th>\r\n          </vs-tr>\r\n        </template>\r\n        <template #tbody>\r\n          <!--<vs-tr :key=\"i\" v-for=\"(tr, i) in tutorials\">-->\r\n          <vs-tr\r\n            :key=\"i\"\r\n            v-for=\"(tr, i) in $vs.getPage(\r\n              $vs.getSearch(tutorials, search1),\r\n              page,\r\n              max\r\n            )\"\r\n            :data=\"tr\"\r\n            :is-selected=\"!!selected1.includes(tr)\"\r\n            not-click-selected1\r\n            open-expand-only-td\r\n          >\r\n            <vs-td>\r\n              {{ tr.name }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ tr.hpNumber }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ tr.orderNumber }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ tr.option }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ tr.time }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ tr.date }}\r\n            </vs-td>\r\n            <vs-td checkbox>\r\n              <vs-checkbox :val=\"tr\" v-model=\"selected1\" />\r\n            </vs-td>\r\n            <vs-td>\r\n              <vs-button danger @click=\"deleteTutorial(tr.key)\"> x </vs-button>\r\n            </vs-td>\r\n\r\n            <template #expand>\r\n              <div class=\"con-content\">\r\n                <vs-table>\r\n                  <template #thead>\r\n                    <vs-tr>\r\n                      <vs-th> Order </vs-th>\r\n                      <vs-th> Description </vs-th>\r\n                      <vs-th> Comment </vs-th>\r\n                      <vs-th> Cost </vs-th>\r\n                      <vs-th> Quantity </vs-th>\r\n                    </vs-tr>\r\n                  </template>\r\n                  <template #tbody>\r\n                    <vs-tr :key=\"j\" v-for=\"(rr, j) in tr.orders\">\r\n                      <vs-td>\r\n                        {{ rr.title }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ rr.description }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ rr.comment }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ rr.cost * rr.quantity }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ rr.quantity }}\r\n                      </vs-td>\r\n                    </vs-tr>\r\n                  </template>\r\n                </vs-table>\r\n              </div>\r\n            </template>\r\n          </vs-tr>\r\n        </template>\r\n        <template #footer>\r\n          <vs-pagination\r\n            v-model=\"page\"\r\n            :length=\"$vs.getLength($vs.getSearch(tutorials, search1), max)\"\r\n          />\r\n        </template>\r\n      </vs-table>\r\n    </div>\r\n    <br />\r\n    <br />\r\n    <br />\r\n    <div class=\"center\" style=\"margin-left: 50px\">\r\n      <H3>Past Orders</H3>\r\n      <vs-table v-model=\"selected\">\r\n        <template #header>\r\n          <vs-input v-model=\"search2\" border placeholder=\"Search\" />\r\n        </template>\r\n        <template #thead>\r\n          <vs-tr>\r\n            <vs-th sort @click=\"diff = $vs.sortData($event, diff, 'name')\">\r\n              Name\r\n            </vs-th>\r\n            <vs-th sort @click=\"diff = $vs.sortData($event, diff, 'hpNumber')\">\r\n              Phone Number\r\n            </vs-th>\r\n            <vs-th\r\n              sort\r\n              @click=\"diff = $vs.sortData($event, diff, 'orderNumber')\"\r\n            >\r\n              Order Number\r\n            </vs-th>\r\n            <vs-th sort @click=\"diff = $vs.sortData($event, diff, 'option')\">\r\n              Option\r\n            </vs-th>\r\n            <vs-th sort @click=\"diff = $vs.sortData($event, diff, 'time')\">\r\n              Time\r\n            </vs-th>\r\n            <vs-th sort @click=\"diff = $vs.sortData($event, diff, 'date')\">\r\n              Date\r\n            </vs-th>\r\n            <vs-th>\r\n              <vs-checkbox\r\n                :indeterminate=\"selected2.length == diff.length\"\r\n                v-model=\"allCheck\"\r\n                @change=\"selected = $vs.checkAll(selected, diff)\"\r\n              />\r\n            </vs-th>\r\n            <vs-th>\r\n              <vs-button danger @click=\"removeAllTutorials\"> X All </vs-button>\r\n            </vs-th>\r\n          </vs-tr>\r\n        </template>\r\n        <template #tbody>\r\n          <!--<vs-tr :key=\"i\" v-for=\"(tr, i) in tutorials\">-->\r\n          <vs-tr\r\n            :key=\"i\"\r\n            v-for=\"(dr, i) in $vs.getPage(\r\n              $vs.getSearch(diff, search2),\r\n              page,\r\n              max2\r\n            )\"\r\n            :data=\"dr\"\r\n            :is-selected=\"!!selected2.includes(dr)\"\r\n            not-click-selected2\r\n            open-expand-only-td\r\n          >\r\n            <vs-td>\r\n              {{ dr.name }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ dr.hpNumber }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ dr.orderNumber }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ dr.option }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ dr.time }}\r\n            </vs-td>\r\n            <vs-td>\r\n              {{ dr.date }}\r\n            </vs-td>\r\n            <vs-td checkbox>\r\n              <vs-checkbox :val=\"dr\" v-model=\"selected\" />\r\n            </vs-td>\r\n            <vs-td>\r\n              <vs-button danger @click=\"deleteTutorial(dr.key)\"> x </vs-button>\r\n            </vs-td>\r\n\r\n            <template #expand>\r\n              <div class=\"con-content\">\r\n                <vs-table>\r\n                  <template #thead>\r\n                    <vs-tr>\r\n                      <vs-th> Order </vs-th>\r\n                      <vs-th> Description </vs-th>\r\n                      <vs-th> Comment </vs-th>\r\n                      <vs-th> Cost </vs-th>\r\n                      <vs-th> Quantity </vs-th>\r\n                    </vs-tr>\r\n                  </template>\r\n                  <template #tbody>\r\n                    <vs-tr :key=\"j\" v-for=\"(dd, j) in dr.orders\">\r\n                      <vs-td>\r\n                        {{ dd.title }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ dd.description }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ dd.comment }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ dd.cost * dd.quantity }}\r\n                      </vs-td>\r\n                      <vs-td>\r\n                        {{ dd.quantity }}\r\n                      </vs-td>\r\n                    </vs-tr>\r\n                  </template>\r\n                </vs-table>\r\n              </div>\r\n            </template>\r\n          </vs-tr>\r\n        </template>\r\n        <template #footer>\r\n          <vs-pagination\r\n            v-model=\"page\"\r\n            :length=\"$vs.getLength($vs.getSearch(diff, search2), max2)\"\r\n          />\r\n        </template>\r\n      </vs-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport TutorialDataService from \"../services/ObtainOrder\";\r\n//import TutorialDetails from \"./Order\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"tutorials-list\",\r\n  //  components: { TutorialDetails },\r\n  data() {\r\n    return {\r\n      tutorials: [],\r\n      diff: [],\r\n      currentTutorial: null,\r\n      currentIndex: -1,\r\n      editActive: false,\r\n      edit: null,\r\n      editProp: {},\r\n      search1: \"\",\r\n      search2: \"\",\r\n      allCheck: false,\r\n      page: 1,\r\n      max: 5,\r\n      max2: 3,\r\n      active: 0,\r\n      selected1: [],\r\n      selected2: [],\r\n    };\r\n  },\r\n  methods: {\r\n    onDataChange(items) {\r\n      let _tutorials = [];\r\n      let _diff = [];\r\n\r\n      items.forEach((item) => {\r\n        let key = item.key;\r\n        let data = item.val();\r\n\r\n        let _orders = [];\r\n        let _diffOrders = [];\r\n        let orderss = data.cart;\r\n        if (data.date === this.currentDate()) {\r\n          orderss.forEach((order) => {\r\n            let data2 = order;\r\n            _orders.push({\r\n              title: data2.title,\r\n              description: data2.description,\r\n              cost: data2.cost,\r\n              comment: data2.comment,\r\n              quantity: data2.quantity,\r\n            });\r\n          });\r\n\r\n          _tutorials.push({\r\n            key: key,\r\n            name: data.name,\r\n            hpNumber: data.hpNumber,\r\n            orderNumber: data.orderNumber,\r\n            option: data.option,\r\n            time: data.time,\r\n            date: data.date,\r\n            orders: _orders,\r\n          });\r\n        } else {\r\n          orderss.forEach((order) => {\r\n            let data2 = order;\r\n            _diffOrders.push({\r\n              title: data2.title,\r\n              description: data2.description,\r\n              cost: data2.cost,\r\n              comment: data2.comment,\r\n              quantity: data2.quantity,\r\n            });\r\n          });\r\n\r\n          _diff.push({\r\n            key: key,\r\n            name: data.name,\r\n            hpNumber: data.hpNumber,\r\n            orderNumber: data.orderNumber,\r\n            option: data.option,\r\n            time: data.time,\r\n            date: data.date,\r\n            orders: _diffOrders,\r\n          });\r\n        }\r\n      });\r\n\r\n      _tutorials.sort(function (a, b) {\r\n        return new Date(a.time) - new Date(b.time);\r\n      });\r\n\r\n      _diff.sort(function (a, b) {\r\n        return new Date(a.time) - new Date(b.time);\r\n      });\r\n\r\n      _diff.sort(function (a, b) {\r\n        return new Date(a.date) - new Date(b.date);\r\n      });\r\n\r\n      this.tutorials = _tutorials;\r\n      this.diff = _diff;\r\n    },\r\n\r\n    refreshList() {\r\n      this.currentTutorial = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActiveTutorial(tutorial, index) {\r\n      this.currentTutorial = tutorial;\r\n      this.currentIndex = index;\r\n    },\r\n\r\n    removeAllTutorials() {\r\n      TutorialDataService.deleteAll(this.$store.getters.user.data.uid)\r\n        .then(() => {\r\n          this.refreshList();\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    deleteTutorial(key) {\r\n      TutorialDataService.delete(this.$store.getters.user.data.uid, key)\r\n        .then(() => {\r\n          this.$emit(\"refreshList\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    currentDate() {\r\n      //do i need date?\r\n      const current = new Date();\r\n      const date = `${current.getFullYear()}/${\r\n        current.getMonth() + 1\r\n      }/${current.getDate()}`;\r\n      return date;\r\n    },\r\n  },\r\n  mounted() {\r\n    TutorialDataService.getAllForStore(this.$store.getters.user.data.uid).on(\r\n      \"value\",\r\n      this.onDataChange\r\n    );\r\n  },\r\n  beforeDestroy() {\r\n    TutorialDataService.getAllForStore(this.$store.getters.user.data.uid).off(\r\n      \"value\",\r\n      this.onDataChange\r\n    );\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      user: \"user\",\r\n    }),\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.coms {\r\n}\r\n/* \"scoped\" attribute limit the CSS to this component only */\r\n.social-button {\r\n  width: 75px;\r\n  background: white;\r\n  padding: 10px;\r\n  border-radius: 100%;\r\n  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.2);\r\n  outline: 0;\r\n  border: 0;\r\n}\r\n.social-button:active {\r\n  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n.social-button img {\r\n  width: 100%;\r\n}\r\n\r\nbody,\r\nhtml {\r\n  margin: 0;\r\n  padding: 0;\r\n  background: #60a3bc !important;\r\n}\r\n.user_card {\r\n  height: 500px;\r\n  width: 600px;\r\n  margin-top: 10px;\r\n  background: white;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  padding: 10px;\r\n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\r\n  -webkit-box-shadow: 0 4px 8px 0 rgba(0, 0