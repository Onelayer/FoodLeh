,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OATA;AAUA,KAZA;AAcA,IAAA,cAdA,4BAcA;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,eAAA,CAAA,KADA;AAEA,QAAA,WAAA,EAAA,KAAA,eAAA,CAAA,WAFA;AAGA,QAAA,IAAA,EAAA,KAAA,eAAA,CAAA,IAHA;AAIA,QAAA,GAAA,EAAA,KAAA,eAAA,CAAA;AAJA,OAAA;AAOA,MAAA,eAAA,CAAA,MAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,eAAA,CAAA,GAAA,EAAA,IAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,yCAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA,KA7BA;AA+BA,IAAA,cA/BA,4BA+BA;AAAA;;AACA,MAAA,eAAA,CAAA,MAAA,CAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,eAAA,CAAA,GAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,aAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA;AAvCA,GAfA;AAwDA,EAAA,OAxDA,qBAwDA;AACA,SAAA,OAAA,GAAA,EAAA;AACA,SAAA,eAAA,qBAAA,KAAA,QAAA;AACA;AA3DA,CAAA","sourcesContent":["<template>\r\n  <div v-if=\"currentTutorial\" class=\"edit-form\">\r\n    <h4>Food Item</h4>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"title\">Title</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"title\"\r\n          v-model=\"currentTutorial.title\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"description\">Description</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"description\"\r\n          v-model=\"currentTutorial.description\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"cost\">Cost</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"cost\"\r\n          v-model=\"currentTutorial.cost\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"image\">\r\n        <img\r\n          v-bind:src=\"currentTutorial.url\"\r\n          width=\"240\"\r\n          height=\"240\"\r\n          alt=\"Food Image\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label><strong>Status:</strong></label>\r\n        {{ currentTutorial.published ? \"Published\" : \"Pending\" }}\r\n      </div>\r\n    </form>\r\n\r\n    <button\r\n      class=\"badge badge-primary mr-2\"\r\n      v-if=\"currentTutorial.published\"\r\n      @click=\"updatePublished(false)\"\r\n    >\r\n      UnPublish\r\n    </button>\r\n    <button\r\n      v-else\r\n      class=\"badge badge-primary mr-2\"\r\n      @click=\"updatePublished(true)\"\r\n    >\r\n      Publish\r\n    </button>\r\n\r\n    <button class=\"badge badge-danger mr-2\" @click=\"deleteTutorial\">\r\n      Delete\r\n    </button>\r\n\r\n    <button type=\"submit\" class=\"badge badge-success\" @click=\"updateTutorial\">\r\n      Update\r\n    </button>\r\n    <p>{{ message }}</p>\r\n  </div>\r\n\r\n  <div v-else>\r\n    <br />\r\n    <p>Please click on a food item...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ObtainStallMenu from \"../services/ObtainStallMenu\";\r\n\r\nexport default {\r\n  name: \"tutorial\",\r\n  props: [\"tutorial\"],\r\n  data() {\r\n    return {\r\n      currentTutorial: null,\r\n      message: \"\",\r\n    };\r\n  },\r\n  watch: {\r\n    tutorial: function (tutorial) {\r\n      this.currentTutorial = { ...tutorial };\r\n      this.message = \"\";\r\n    },\r\n  },\r\n  methods: {\r\n    updatePublished(status) {\r\n      ObtainStallMenu.update(this.$store.getters.user.data.uid, this.currentTutorial.key, {\r\n        published: status,\r\n      })\r\n        .then(() => {\r\n          this.currentTutorial.published = status;\r\n          this.message = \"The status was updated successfully!\";\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    updateTutorial() {\r\n      const data = {\r\n        title: this.currentTutorial.title,\r\n        description: this.currentTutorial.description,\r\n        cost: this.currentTutorial.cost,\r\n        url: this.currentTutorial.url,\r\n      };\r\n\r\n      ObtainStallMenu.update(this.$store.getters.user.data.uid, this.currentTutorial.key, data)\r\n        .then(() => {\r\n          this.message = \"The food item was updated successfully!\";\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    deleteTutorial() {\r\n      ObtainStallMenu.delete(this.$store.getters.user.data.uid, this.currentTutorial.key)\r\n        .then(() => {\r\n          this.$emit(\"refreshList\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.message = \"\";\r\n    this.currentTutorial = { ...this.tutorial };\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}