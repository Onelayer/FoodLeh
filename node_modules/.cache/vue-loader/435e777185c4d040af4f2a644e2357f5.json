  console.log(\"Created new item successfully!\");\r\n          this.submitted = true;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    newTutorial() {\r\n      this.submitted = false;\r\n      this.tutorial = {\r\n        title: \"\",\r\n        description: \"\",\r\n        cost: \"\",\r\n        url: \"\",\r\n        published: false,\r\n      };\r\n    },\r\n    getUrl() {\r\n      this.url = \"fornow\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.coms {\r\n  padding-top: 50px;\r\n  padding-left: 70px;\r\n}\r\n</style>\r\n"]}]}